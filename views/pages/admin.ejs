    <link
      href="https://cdn.quilljs.com/1.0.0/quill.snow.css"
      rel="stylesheet"
    />
    <!-- Include the Quill library -->
    <script src="https://cdn.quilljs.com/1.0.0/quill.js"></script>

    <script type="text/javascript" src="/falabella.js"></script>

    <script type="text/javascript" src="/f.control.editor.js"></script>
