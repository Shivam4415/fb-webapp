  <body>
    <div class="uk-height-4-5 uk-background-muted">
      <div class="uk-margin-large-top uk-margin-auto uk-width-1-3@s uk-padding-bottom uk-height-1-1">
        <input id="sendTo" type="email" placeholder="Sent to" class="uk-input uk-margin-top">
        <input id="subject" type="text" placeholder="Subject" class="uk-input uk-margin-top uk-margin-bottom"> 
        <div
          id="editorContainer"
          class="uk-background-default"
        style="height: 200px!important;"></div>

        <input id="sendMail" value="Submit" class="uk-button uk-button-primary uk-margin-top"> 

      </div>
    </div>
  </body>
</html>
